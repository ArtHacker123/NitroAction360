<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ui_layout"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <com.google.vrtoolkit.cardboard.CardboardView
        android:id="@+id/cardboard_view"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true" />

    <com.nitro888.nitroaction360.cardboard.NACardboardOverlayView
        android:id="@+id/overlay"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true" />


    <com.nitro888.nitroaction360.nitroaction.NAGUIRelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/GUI">

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Setting"
            android:columnCount="3"
            android:rowCount="3">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_sbs_3d_"
                android:layout_row="0"
                android:layout_column="0"
                android:onClick="onGUIButtonClick"
                android:src="@drawable/ic_view_week_black_48dp" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_dome_"
                android:layout_row="0"
                android:layout_column="1"
                android:onClick="onGUIButtonClick"
                android:src="@drawable/ic_panorama_fisheye_black_48dp" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_screen_sizeup_"
                android:layout_row="0"
                android:layout_column="2"
                android:src="@drawable/ic_exposure_plus_1_black_48dp"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_2d_"
                android:src="@drawable/ic_crop_16_9_black_48dp"
                android:onClick="onGUIButtonClick"
                android:layout_row="1"
                android:layout_column="0" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_back_"
                android:src="@drawable/ic_cancel_black_48dp"
                android:onClick="onGUIButtonClick"
                android:layout_row="1"
                android:layout_column="1" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_screen_sizeReset_"
                android:layout_row="1"
                android:layout_column="2"
                android:onClick="onGUIButtonClick"
                android:src="@drawable/ic_exposure_zero_black_48dp" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_tb_3d_"
                android:layout_row="2"
                android:layout_column="0"
                android:onClick="onGUIButtonClick"
                android:src="@drawable/ic_view_stream_black_48dp" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_panorama_l"
                android:src="@drawable/ic_panorama_horizontal_black_48dp"
                android:onClick="onGUIButtonClick"
                android:layout_row="2"
                android:layout_column="1" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_screen_sizedown_"
                android:layout_row="2"
                android:layout_column="2"
                android:src="@drawable/ic_exposure_minus_1_black_48dp"
                android:onClick="onGUIButtonClick" />
        </GridLayout>

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rowCount="3"
            android:columnCount="3"
            android:id="@+id/Browser">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_left"
                android:src="@drawable/ic_keyboard_arrow_left_black_48dp"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_close"
                android:src="@drawable/ic_cancel_black_48dp"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_right"
                android:src="@drawable/ic_keyboard_arrow_right_black_48dp"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_file01"
                android:src="@drawable/ic_play_circle_outline_black_48dp"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_file02"
                android:src="@drawable/ic_play_circle_outline_black_48dp"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_file03"
                android:src="@drawable/ic_play_circle_outline_black_48dp"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_file04"
                android:src="@drawable/ic_play_circle_outline_black_48dp"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_file05"
                android:src="@drawable/ic_play_circle_outline_black_48dp"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_file06"
                android:src="@drawable/ic_play_circle_outline_black_48dp"
                android:onClick="onGUIButtonClick" />
        </GridLayout>

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Player"
            android:rowCount="3"
            android:columnCount="3">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_folder"
                android:src="@drawable/ic_folder_black_48dp"
                android:layout_row="0"
                android:layout_column="0"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_screen_up"
                android:layout_row="0"
                android:layout_column="1"
                android:src="@drawable/ic_vertical_align_top_black_48dp"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_setting"
                android:src="@drawable/ic_settings_black_48dp"
                android:layout_row="0"
                android:layout_column="2"
                android:layout_alignBottom="@+id/playerR"
                android:layout_toRightOf="@+id/playerR"
                android:layout_toEndOf="@+id/playerR"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_fast_rewind"
                android:src="@drawable/ic_fast_rewind_black_48dp"
                android:layout_row="1"
                android:layout_column="0"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_play"
                android:src="@drawable/ic_play_arrow_black_48dp"
                android:layout_row="1"
                android:layout_column="1"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_fast_forward"
                android:src="@drawable/ic_fast_forward_black_48dp"
                android:layout_row="1"
                android:layout_column="2"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_skip_previous"
                android:src="@drawable/ic_skip_previous_black_48dp"
                android:layout_row="2"
                android:layout_column="0"
                android:layout_below="@+id/playerR"
                android:layout_alignRight="@+id/playerR"
                android:layout_alignEnd="@+id/playerR"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_screen_down"
                android:layout_row="2"
                android:layout_column="1"
                android:src="@drawable/ic_vertical_align_bottom_black_48dp"
                android:onClick="onGUIButtonClick" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_skip_next"
                android:src="@drawable/ic_skip_next_black_48dp"
                android:layout_row="2"
                android:layout_column="2"
                android:onClick="onGUIButtonClick" />

        </GridLayout>
    </com.nitro888.nitroaction360.nitroaction.NAGUIRelativeLayout>


    <!--
    <fragment
        android:name="com.google.android.youtube.player.YouTubePlayerFragment"
        android:id="@+id/youtubeplayerfragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    -->


</RelativeLayout>
